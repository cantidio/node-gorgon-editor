{
  "name": "gorgon-editor",
  "version": "0.0.1",
  "description": "2D Game Sprite & Animation Editor",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cantidio/node-gorgon-editor"
  },
  "scripts": {
    "start-dev": "./node_modules/.bin/webpack-dev-server --inline --content-base --colors",
    "test": "jest --verbose --coverage"
  },
  "jest": {
    "testDirectoryName": "tests",
    "testPathDirs": [
      "tests"
    ],
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "author": "Cantidio Fontes <aniquilatorbloody@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "^0.9.2",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
